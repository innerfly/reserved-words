{
    "name": "sspat/reserved-words",
    "description": "Validates strings against reserved words of a specific php version",
    "type": "library",
    "version": "1.0.0",
    "keywords": [
        "reserved",
        "words",
        "php"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Patrik Foldes",
            "email": "studio22@mail.ru"
        }
    ],
    "require": {
        "php": ">=7.2.0"
    },
    "require-dev": {
        "doctrine/coding-standard": "^7.0",
        "phpunit/phpunit": "^8",
        "squizlabs/php_codesniffer": "^3.5",
        "vimeo/psalm": "^3.8"
    },
    "autoload": {
        "psr-4": {
            "sspat\\ReservedWords\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "sspat\\ReservedWords\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist"
    },
    "scripts": {
        "cs": "phpcs",
        "csfix": "phpcbf",
        "psalm": "psalm",
        "test": "phpunit",
        "all": "composer psalm && composer test && composer cs"
    }
}
